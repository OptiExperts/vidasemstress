.sticky-chat .chat-button{display:block;align-items:center;margin-top:5px;padding:10px;border-radius:14px;background-color:#bac1d1b8;backdrop-filter:blur(5px);overflow:hidden;font-size:12px;font-family:var(--text-font-family);font-weight:400;}
